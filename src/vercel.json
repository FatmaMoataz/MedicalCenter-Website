{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "x-vercel-force-rewrite": "1"
      },
      "dest": "/api/$1.js"
    }
  ]
}
